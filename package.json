{
  "name": "wtfwith",
  "version": "2.0.1",
  "description": "Check your lock file for repeated dependencies with different versions",
  "keywords": [
    "npx",
    "dependencies"
  ],
  "main": "index.js",
  "scripts": {
    "test": "mocha --exit",
    "cover": "nyc mocha -t 10000 --exit",
    "check_coverage": "nyc check-coverage --lines 90 --functions 90 --branches 80",
    "coverage": "nyc npm test",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "lint": "./node_modules/.bin/eslint index.js lib.js test"
  },
  "bin": {
    "wtfwith": "index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jehy/wtfwith"
  },
  "homepage": "https://github.com/jehy/wtfwith",
  "author": {
    "name": "Jehy",
    "email": "npm@jehy.ru",
    "url": "https://github.com/jehy/"
  },
  "license": "MIT",
  "dependencies": {
    "check-exact": "1.1.0",
    "colors": "1.3.3",
    "commander": "2.19.0",
    "debug": "4.1.1",
    "semver": "5.6.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.12.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0"
  },
  "engines": {
    "node": ">= 6"
  }
}
